{
  "htmlWhitespaceSensitivity": "ignore",
  "singleQuote": true,
  "bracketSameLine": false,
  "plugins": ["prettier-plugin-tailwindcss"],
  "printWidth": 80,
  "trailingComma": "none",
  "semi": true,
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "singleAttributePerLine": false,
        "parser": "html",
        "htmlVoidTags": false
      }
    }
  ]
}
